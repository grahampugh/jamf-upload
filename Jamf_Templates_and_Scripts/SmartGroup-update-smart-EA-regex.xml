<?xml version="1.0" encoding="UTF-8"?>
<computer_group>
	<name>%GROUP_NAME%</name>
	<is_smart>true</is_smart>
	<criteria>
		<criterion>
			<name>Application Title</name>
			<priority>0</priority>
			<and_or>and</and_or>
			<search_type>is</search_type>
			<value>%JSS_INVENTORY_NAME%</value>
		</criterion>
		<criterion>
			<name>%EXTENSION_ATTRIBUTE_NAME%</name>
			<priority>1</priority>
			<and_or>and</and_or>
			<search_type>does not match regex</search_type>
			<value>%version_regex%</value>
		</criterion>
		<criterion>
			<name>%EXTENSION_ATTRIBUTE_NAME%</name>
			<priority>2</priority>
			<and_or>and</and_or>
			<search_type>does not match regex</search_type>
			<value>^$</value>
		</criterion>
		<criterion>
			<name>Computer Group</name>
			<priority>3</priority>
			<and_or>and</and_or>
			<search_type>member of</search_type>
			<value>%TESTING_GROUP_NAME%</value>
		</criterion>
	</criteria>
</computer_group>
